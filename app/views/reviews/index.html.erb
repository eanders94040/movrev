<h1>Listing reviews</h1>

<table>
  <tr>
    <th>Email</th>
    <th>Review Date</th>
    <th>Rating</th>
    <th>Movie</th>
    <th>Comment</th>
  </tr>
 
  <% @reviews.each do |review| %>
    <tr>
      <td><%= review.email %></td>
      <td><%= review.review_date %></td>
      <td><%= review.rating %></td>
      <td><%= review.movie_id %> <%= review.movie %></td>
      <td><%= review.comment %></td>
    </tr>
  <% end %>
</table>
